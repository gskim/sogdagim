{
    "extends": ["../../tslint.json"],
    "rules": {
        "variable-name": [
            false,
            "allow-leading-underscore",
            "allow-pascal-case",
            "allow-snake-case",
            "allow-trailing-underscore",
            "ban-keywords",
            "require-const-for-all-caps"
        ],
        "object-literal-shorthand": true
    }
}